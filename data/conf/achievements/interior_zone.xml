<?xml version="1.0" encoding="UTF-8"?>
<achievements xmlns="stendhal" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="stendhal ../achievements.xsd ">

  <achievement name="interior zone achievement">
    <type class="achievement"/>
    <category value = "INTERIOR_ZONE"/>
    <implementation class-name="games.stendhal.server.core.rp.achievement.factory.InteriorZoneAchievementFactory"/>
    <identifier value="zone.interior.semos"/>
    <title value="Home maker"/>
    <description value="Visit all interior zones in the Semos region"/>
    <score value="Achievement.MEDIUM_BASE_SCORE"/>
    <active value="true"/>
    <conditions>
      <condition>
        <method value="PlayerVisitedZonesInRegionCondition"/>
        <parameter value="semos"/>
        <parameter value="Boolean.FALSE"/>
      </condition>
    </conditions>
  </achievement>

</achievements>